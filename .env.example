# Database
MYSQL_VERSION=8.4
MYSQL_DOCKER_IMAGE=mysql:${MYSQL_VERSION}
DB_ROOT_PASSWORD=password
DB_NAME=db
DB_USER=user
DB_PASSWORD=password
DB_PORT=13306

# Dbmate
DATABASE_URL=mysql://${DB_USER}:${DB_PASSWORD}@127.0.0.1:${DB_PORT}/${DB_NAME}

# R2DBC
R2DBC_URL=r2dbc:${DATABASE_URL}

# Testcontainers
TESTCONTAINERS_URL=r2dbc:tc:mysql:///${DB_NAME}?TC_IMAGE_TAG=${MYSQL_VERSION}&TC_REUSABLE=true
